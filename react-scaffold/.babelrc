{
  "presets": [
    "es2015",
    "react",
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    "react-hot-loader/babel",
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ]
  ],
  "comments": false,
  "env": {
    "production": {
      "presets": [
        "react-optimize"
      ]
    }
  }
}